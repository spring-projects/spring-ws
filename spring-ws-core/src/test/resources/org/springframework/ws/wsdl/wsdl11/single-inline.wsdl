            <wsdl:output name="GetOrderResponse">
                <soap:body use="literal"/>
            </wsdl:output>
            <wsdl:fault name="GetOrderFault">
                <soap:fault name="GetOrderFault" use="literal"/>
            </wsdl:fault>
        </wsdl:operation>
    </wsdl:binding>
    <wsdl:service name="OrderService">
