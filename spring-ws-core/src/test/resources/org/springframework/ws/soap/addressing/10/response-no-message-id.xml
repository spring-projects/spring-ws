              xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/08/addressing/">
    <env:Header/>
    <env:Body>
        <env:Fault>
            <env:Code>
                <env:Value>env:Sender</env:Value>
                <env:Subcode>
                    <env:Value>wsa:MessageAddressingHeaderRequired</env:Value>
                </env:Subcode>
